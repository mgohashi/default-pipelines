pipeline {
  parameters {
    string(name: 'PROJECT_NAME', description: 'What is the project name?')
    string(name: 'APP_NAME', description: 'What is the application name?')
  }
  agent any
  stages {
    stage('Validate') {
      input {
          message "Should we continue?"
          ok "Yes, we should."
          submitter "aprovadores"
          submitterParameter "USERNAME" 
      }
      steps {
        script {
          echo "HELLO ${USERNAME}"
        }
      }
    }
  }
}